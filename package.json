{
  "name": "remix-on-pages",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "test": "bun test",
    "typecheck": "tsc",
    "generate": "drizzle-kit generate:sqlite",
